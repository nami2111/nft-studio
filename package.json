{
	"name": "nft-studio",
	"version": "0.5.2",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "tsc --noEmit && vite build && npm run postbuild",
		"postbuild": "node scripts/remove-comments.js",
		"prepare": "svelte-kit sync",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"standardize-comments": "node scripts/standardize-comments.js",
		"test": "vitest run",
		"test:watch": "vitest",
		"test:coverage": "vitest run --coverage",
		"postinstall:copy-auth": "node -e \"require('fs').cpSync('node_modules/@junobuild/core/dist/workers/', './static/workers', {recursive: true});\"",
		"postinstall": "npm run postinstall:copy-auth",
		"verify-lockfile": "bash ./scripts/verify-lockfile.sh",
		"verify-lock": "npm run verify-lockfile",
		"verify-lockfile-ci": "bash ./scripts/verify-lockfile.sh",
		"lint-ci": "npm run lint",
		"test:ci": "vitest run --reporter=junit --outdir .svelte-kit/test-results"
	},
	"devDependencies": {
		"@eslint/js": "^9.39.2",
		"@internationalized/date": "^3.10.1",
		"@junobuild/config": "^2.8.0",
		"@junobuild/vite-plugin": "^4.4.0",
		"@lucide/svelte": "^0.562.0",
		"@sveltejs/adapter-static": "^3.0.10",
		"@sveltejs/enhanced-img": "^0.9.2",
		"@sveltejs/kit": "^2.49.2",
		"@sveltejs/vite-plugin-svelte": "^6.2.1",
		"@tailwindcss/postcss": "^4.1.18",
		"@tailwindcss/vite": "^4.1.18",
		"@testing-library/jest-dom": "^6.9.1",
		"@testing-library/svelte": "^5.3.1",
		"@types/eslint": "^9.6.1",
		"@types/node": "^25.0.3",
		"@vitest/coverage-v8": "^4.0.16",
		"clsx": "^2.1.1",
		"dompurify": "^3.3.1",
		"eslint": "^9.39.2",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^3.13.1",
		"globals": "^16.5.0",
		"jsdom": "^27.4.0",
		"mode-watcher": "^1.1.0",
		"postcss": "^8.5.6",
		"prettier": "3.7.4",
		"prettier-plugin-svelte": "^3.4.1",
		"prettier-plugin-tailwindcss": "^0.7.2",
		"rollup-plugin-visualizer": "^6.0.5",
		"svelte": "^5.46.1",
		"svelte-check": "^4.3.5",
		"svelte-sonner": "^1.0.7",
		"tailwind-merge": "^3.4.0",
		"tailwind-variants": "^3.2.2",
		"tailwindcss": "^4.1.18",
		"tslib": "^2.8.1",
		"tw-animate-css": "^1.4.0",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.51.0",
		"vite": "^7.3.0",
		"vite-plugin-pwa": "^1.2.0",
		"vitest": "^4.0.16"
	},
	"type": "module",
	"overrides": {
		"cookie": "^0.7.0"
	},
	"dependencies": {
		"@junobuild/analytics": "^0.2.10",
		"@junobuild/core": "^3.3.2",
		"@junobuild/functions": "^0.5.3",
		"@zip.js/zip.js": "^2.8.11",
		"bits-ui": "^2.14.4",
		"idb": "^8.0.3",
		"jszip": "^3.10.1",
		"svelte-dnd-action": "^0.9.69",
		"zod": "^4.3.2"
	}
}
