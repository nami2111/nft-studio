{
	"name": "nft-studio",
	"version": "0.4.5",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "tsc --noEmit && vite build && npm run postbuild",
		"postbuild": "node scripts/remove-comments.js",
		"prepare": "svelte-kit sync",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"standardize-comments": "node scripts/standardize-comments.js",
		"test": "vitest run",
		"test:watch": "vitest",
		"test:coverage": "vitest run --coverage",
		"postinstall:copy-auth": "node -e \"require('fs').cpSync('node_modules/@junobuild/core/dist/workers/', './static/workers', {recursive: true});\"",
		"postinstall": "npm run postinstall:copy-auth",
		"verify-lockfile": "bash ./scripts/verify-lockfile.sh",
		"verify-lock": "npm run verify-lockfile",
		"verify-lockfile-ci": "bash ./scripts/verify-lockfile.sh",
		"lint-ci": "npm run lint",
		"test:ci": "vitest run --reporter=junit --outdir .svelte-kit/test-results"
	},
	"devDependencies": {
		"@eslint/js": "^9.37.0",
		"@internationalized/date": "^3.10.0",
		"@junobuild/config": "^2.5.0",
		"@junobuild/vite-plugin": "^4.3.0",
		"@lucide/svelte": "^0.544.0",
		"@sveltejs/adapter-static": "^3.0.10",
		"@sveltejs/enhanced-img": "^0.8.4",
		"@sveltejs/kit": "^2.47.0",
		"@sveltejs/vite-plugin-svelte": "^6.2.1",
		"@tailwindcss/postcss": "^4.1.14",
		"@tailwindcss/vite": "^4.1.14",
		"@testing-library/jest-dom": "^6.9.1",
		"@testing-library/svelte": "^5.2.8",
		"@types/eslint": "^9.6.1",
		"@vitest/coverage-v8": "^3.2.4",
		"clsx": "^2.1.1",
		"eslint": "^9.37.0",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^3.12.4",
		"globals": "^16.4.0",
		"jsdom": "^27.0.0",
		"mode-watcher": "^1.1.0",
		"postcss": "^8.5.6",
		"prettier": "^3.6.2",
		"prettier-plugin-svelte": "^3.4.0",
		"prettier-plugin-tailwindcss": "^0.6.14",
		"rollup-plugin-visualizer": "^6.0.5",
		"svelte": "^5.40.1",
		"svelte-check": "^4.3.3",
		"svelte-sonner": "^1.0.5",
		"tailwind-merge": "^3.3.1",
		"tailwind-variants": "^3.1.1",
		"tailwindcss": "^4.1.14",
		"tslib": "^2.8.1",
		"tw-animate-css": "^1.4.0",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.46.1",
		"vite": "^7.1.10",
		"vite-plugin-pwa": "^1.1.0",
		"vitest": "^3.2.4"
	},
	"type": "module",
	"overrides": {
		"cookie": "^0.7.0"
	},
	"dependencies": {
		"@humanspeak/svelte-virtual-list": "^0.3.5",
		"@junobuild/analytics": "^0.2.8",
		"@junobuild/core": "^2.2.0",
		"@junobuild/functions": "^0.2.7",
		"bits-ui": "^2.11.7",
		"idb": "^8.0.3",
		"jszip": "^3.10.1",
		"lucide-svelte": "^0.544.0",
		"svelte-dnd-action": "^0.9.65",
		"svelte-headlessui": "^0.0.46",
		"svelte-virtual": "^0.6.3",
		"zod": "^4.1.12"
	}
}
