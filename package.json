{
	"name": "nft-studio",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "tsc --noEmit && vite build && npm run postbuild",
		"postbuild": "node scripts/remove-comments.js",
		"prepare": "svelte-kit sync",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"standardize-comments": "node scripts/standardize-comments.js",
		"test": "vitest run --reporter=json -t \"Test name\"",
		"postinstall:copy-auth": "node -e \"require('fs').cpSync('node_modules/@junobuild/core/dist/workers/', './static/workers', {recursive: true});\"",
		"postinstall": "npm run postinstall:copy-auth",
		"verify-lockfile": "bash ./scripts/verify-lockfile.sh",
		"verify-lock": "npm run verify-lockfile",
		"verify-lockfile-ci": "bash ./scripts/verify-lockfile.sh",
		"lint-ci": "npm run lint",
		"test:ci": "vitest run --reporter=json"
	},
	"devDependencies": {
		"@eslint/js": "^9.35.0",
		"@internationalized/date": "^3.9.0",
		"@junobuild/config": "^1.1.3",
		"@junobuild/vite-plugin": "^4.1.4",
		"@lucide/svelte": "^0.515.0",
		"@sveltejs/adapter-static": "^3.0.9",
		"@sveltejs/kit": "^2.37.1",
		"@sveltejs/vite-plugin-svelte": "^5.1.1",
		"@tailwindcss/postcss": "^4.1.13",
		"@tailwindcss/vite": "^4.1.13",
		"@types/eslint": "^9.6.1",
		"clsx": "^2.1.1",
		"eslint": "^9.35.0",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^3.12.1",
		"globals": "^16.3.0",
		"mode-watcher": "^1.1.0",
		"postcss": "^8.5.6",
		"prettier": "^3.6.2",
		"prettier-plugin-svelte": "^3.4.0",
		"prettier-plugin-tailwindcss": "^0.6.14",
		"rollup-plugin-visualizer": "^6.0.3",
		"svelte": "^5.38.7",
		"svelte-check": "^4.3.1",
		"svelte-sonner": "^1.0.5",
		"tailwind-merge": "^3.3.1",
		"tailwind-variants": "^1.0.0",
		"tailwindcss": "^4.1.13",
		"tslib": "^2.8.1",
		"tw-animate-css": "^1.3.8",
		"typescript": "^5.9.2",
		"typescript-eslint": "^8.42.0",
		"vite": "^6.3.5"
	},
	"type": "module",
	"overrides": {
		"cookie": "^0.7.0"
	},
	"dependencies": {
		"@junobuild/core": "^1.2.1",
		"@junobuild/functions": "^0.2.7",
		"bits-ui": "^2.9.6",
		"jszip": "^3.10.1",
		"lucide-svelte": "^0.536.0",
		"shadcn-svelte": "^1.0.7",
		"svelte-dnd-action": "^0.9.65",
		"svelte-headlessui": "^0.0.46"
	}
}
